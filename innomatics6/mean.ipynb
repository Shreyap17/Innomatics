        "import numpy\n",
        "\n",
        "N,M = map(int, input().split())\n",
        "l = []\n",
        "\n",
        "for i in range(N):\n",
        "    a = list(map(int, input().split()))\n",
        "    l.append(a)\n",
        "\n",
        "l = numpy.array(l)\n",
        "\n",
        "numpy.set_printoptions(legacy='1.13')\n",
        "print(numpy.mean(l, axis =1))\n",
        "print(numpy.var(l, axis =0))\n",
        "print(numpy.std(l))\n"
      ],
