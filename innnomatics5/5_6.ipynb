        "import re\n",
        "\n",
        "def change(match):\n",
        "    symb = match.group(0)\n",
        "    \n",
        "    if symb == \"&&\":\n",
        "        return \"and\"\n",
        "    elif symb == \"||\":\n",
        "        return \"or\"\n",
        "    \n",
        "n = int(input().strip())\n",
        "for _ in range(n):\n",
        "    print(re.sub(r'(?<= )(&&|\\|\\|)(?= )', change, input()))\n"
      ],
