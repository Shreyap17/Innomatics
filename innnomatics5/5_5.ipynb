        "import re\n",
        "\n",
        "S, k = input(), input()\n",
        "matches = re.finditer(r'(?=(' + k + '))', S)\n",
        "\n",
        "any1 = False\n",
        "for match in matches:\n",
        "    any1 = True\n",
        "    print((match.start(1), match.end(1) - 1))\n",
        "\n",
        "if any1 == False:\n",
        "    print((-1, -1))\n"
