      "source": [
        "\n",
        "import math\n",
        "\n",
        "\n",
        "var = 0.12\n",
        "ans1 = 0\n",
        "for i in range(0, 3):\n",
        "    ans1 += math.factorial(10)/math.factorial(i)/math.factorial(10-i) * var**i * (1-var)**(10-i)\n",
        "    if i == 1:\n",
        "        ans2 = 1 - ans1\n",
        "\n",
        "print(round(ans1, 3))\n",
        "print(round(ans2, 3))\n"
      ],
