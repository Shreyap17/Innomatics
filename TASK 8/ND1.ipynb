      "source": [
        "import math\n",
        "\n",
        "\n",
        "def cdf(x, mean, std):\n",
        "    return 1/2*(1+math.erf((x-mean) / std / 2**(1/2)))\n",
        "\n",
        "\n",
        "mean = 20\n",
        "std = 2\n",
        "\n",
        "print(round(cdf(19.5, mean, std), 3))\n",
        "\n",
        "print(round(cdf(22, mean, std) - cdf(20, mean, std), 3))\n"
      ],
